{
	"name": "x3p0-dev/x3p0-breadcrumbs",
	"type": "wordpress-plugin",
	"license": "GPL-3.0-or-later",
	"scripts": {
		"build": [
			"composer update --no-dev",
			"composer dump-autoload -a --no-dev"
		],
		"dev": [
			"composer update",
			"composer dump-autoload"
		],
		"zip": "composer archive --file=x3p0-breadcrumbs"
	},
	"autoload": {
		"psr-4": {
			"X3P0\\Breadcrumbs\\": "src/"
		},
		"files" : [
			"src/functions-helpers.php"
		],
		"exclude-from-classmap": [
			"vendor/"
		]
	},
	"require": {
                "php": ">=8.0"
	},
	"require-dev": {
		"symfony/var-dumper": "^7.1"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"config": {
		"archive-dir": "./",
		"archive-format": "zip"
	},
	"archive": {
		"exclude": [
			"/vendor",
			"!/vendor/autoload.php",
			"!/vendor/composer/"
		]
	},
	"authors": [
		{
			"name": "Justin Tadlock",
			"email": "justintadlock@gmail.com"
		}
	]
}
